{
    "name": "rising-bsm-backend",
    "version": "1.0.0",
    "description": "Backend for Rising BSM business management system",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only server.ts",
        "build": "tsc -p tsconfig.json",
        "prisma:generate": "prisma generate",
        "dev:ts": "ts-node-dev --respawn --transpile-only server.ts",
        "build:watch": "tsc -w",
        "migrate": "prisma migrate dev",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@dr.pogodin/csurf": "^1.12.3",
        "@prisma/client": "^5.0.0",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "connect-flash": "^0.1.1",
        "connect-pg-simple": "^8.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dotenv": "^16.0.3",
        "exceljs": "^4.3.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-session": "^1.17.3",
        "express-validator": "^7.0.1",
        "helmet": "^6.1.5",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "pdfkit": "^0.13.0",
        "pg": "^8.10.0",
        "validator": "^13.9.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/connect-flash": "^0.0.37",
        "@types/connect-pg-simple": "^7.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.15.11",
        "@types/pdfkit": "^0.12.10",
        "@types/pg": "^8.6.6",
        "@types/validator": "^13.7.17",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "eslint": "^8.38.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7",
        "prisma": "^5.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
}
