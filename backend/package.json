{
    "name": "rising-bsm-api",
    "version": "1.0.0",
    "type": "module",
    "description": "Rising BSM API",
    "main": "dist/index.js",
    "scripts": {
        "prisma:generate": "prisma generate",
        "build": "npm run openapi:bundle && tsc -p tsconfig.json",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "build:watch": "tsc -w",
        "db:seed": "prisma migrate dev && tsx prisma/seed.ts",
        "migrate": "prisma migrate dev",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "openapi:validate": "node scripts/validate-openapi.js",
        "openapi:bundle": "node scripts/bundle-openapi.js",
        "openapi:serve": "redoc-cli serve dist/swagger.json --port 8080",
        "prestart": "node scripts/ensure-swagger.js",
        "predev": "node scripts/ensure-swagger.js"
    },
    "dependencies": {
        "@dr.pogodin/csurf": "^1.12.3",
        "@faker-js/faker": "^9.6.0",
