        "test:watch": "jest --watch",
        "build-js": "uglifyjs public/scripts/scripts.js -o public/scripts/scripts.min.js"
    },
    "keywords": [
        "business",
        "service",
        "management",
        "crm",
        "node",
        "express"
    ],
    "dependencies": {
        "@dr.pogodin/csurf": "^1.14.1",
        "axios": "^1.8.2",
        "bcrypt": "^5.1.1",
        "bcryptjs": "^2.4.3",
        "connect-flash": "^0.1.1",
        "csurf": "^1.10.0",
        "date-fns": "^4.1.0",
        "ejs": "^3.1.10",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "express-rate-limit": "^7.5.0",
        "express-session": "^1.18.1",
        "helmet": "^8.0.0",
        "multer": "^1.4.5-lts.1",
        "pdfkit": "^0.16.0",
        "pg": "^8.13.3",
        "slugify": "^1.6.6",
        "validator": "^13.12.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "jest-mock-extended": "^4.0.0-beta1",
        "supertest": "^6.3.4",
        "uglify-js": "^3.19.3"
    }
