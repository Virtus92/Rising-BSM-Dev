{
    "name": "rising-bsm",
    "version": "1.0.0",
    "description": "Business Service Management System",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js",
        "test": "jest",
        "test:unit": "jest 'tests/utils|tests/services|tests/controllers' --testPathIgnorePatterns=integration",
        "test:integration": "TEST_DB=true jest 'tests/integration'",
